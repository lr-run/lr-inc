{
  "name": "lr-inc",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "prettier --check \"**/*.{js,ts,jsx,tsx}\"",
    "format:fix": "prettier --write \"**/*.{js,ts,jsx,tsx}\"",
    "test": "turbo test",
    "test:ci": "turbo test:ci",
    "db:migrate": "turbo db:migrate",
    "inngest": "pnpm dlx inngest-cli@latest dev --no-discovery -u http://localhost:8000/inngest"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.15.7",
  "engines": {
    "node": ">=18"
  }
}
